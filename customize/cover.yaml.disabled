###########################################################
# Customize: Cover
# Uses: https://github.com/andrey-git/home-assistant-custom-ui
###########################################################

cover.garage_door:
  custom_ui_state_card: custom_cover
  state_card_mode: break-slider
  show_last_changed: false
  extra_badge:
    entity_id: sensor.my_sensor
    attribute: power_consumption
    unit: "%"
    blacklist_states: 0
